{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/users/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/users/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/users",
  "projects": {
    "nest-aws-lambda-serverless-monorepo": {
      "type": "application",
      "root": "apps/nest-aws-lambda-serverless-monorepo",
      "entryFile": "main",
      "sourceRoot": "apps/nest-aws-lambda-serverless-monorepo/src",
      "compilerOptions": {
        "tsConfigPath": "apps/nest-aws-lambda-serverless-monorepo/tsconfig.app.json"
      }
    },
    "create-user": {
      "type": "application",
      "root": "apps/create-user",
      "entryFile": "main",
      "sourceRoot": "apps/create-user/src",
      "compilerOptions": {
        "tsConfigPath": "apps/create-user/tsconfig.app.json"
      }
    },
    "commons": {
      "type": "library",
      "root": "libs/commons",
      "entryFile": "index",
      "sourceRoot": "libs/commons/src",
      "compilerOptions": {
        "tsConfigPath": "libs/commons/tsconfig.lib.json"
      }
    },
    "users": {
      "type": "application",
      "root": "apps/users",
      "entryFile": "main",
      "sourceRoot": "apps/users/src",
      "compilerOptions": {
        "tsConfigPath": "apps/users/tsconfig.app.json"
      }
    },
    "config": {
      "type": "library",
      "root": "libs/config",
      "entryFile": "index",
      "sourceRoot": "libs/config/src",
      "compilerOptions": {
        "tsConfigPath": "libs/config/tsconfig.lib.json"
      }
    }
  }
}
